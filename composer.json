{
    "name": "dew-serverless/laravel-mns-driver",
    "description": "Alibaba Cloud MNS driver for Laravel queue.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Li Zhineng",
            "email": "im@zhineng.li"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^10.0",
        "illuminate/queue": "^10.0",
        "dew-serverless/mns": "^1.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.23",
        "mockery/mockery": "^1.6",
        "laravel/pint": "^1.13",
        "rector/rector": "^0.18.6",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Dew\\MnsDriver\\": "src/"
        }
    },
    "scripts": {
        "format": "pint",
        "refactor": "rector",
        "test:style": "pint --test",
        "test:static": "rector --dry-run",
        "test:cases": "pest",
        "test": [
            "@test:style",
            "@test:static",
            "@test:cases"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dew\\MnsDriver\\MnsServiceProvider"
            ]
        }
    }
}
